.mui-bar {
	/* Safari 5.1 - 6.0 */
	background: -webkit-linear-gradient(#ACC659, #547B10);
	background: -moz-linear-gradient(#ACC659, #547B10);
	background: -o-linear-gradient(#ACC659, #547B10);
	background: -webkit-gradient(linear, 0 0, 0 100%, from(#ACC659), to(#547B10));
	background: linear-gradient(#ACC659, #547B10);
	/* Opera 11.1 - 12.0 */
	/* Firefox 3.6 - 15 */
	/* 标准的语法 */
}

.mui-icon {
	color: #309D02;
}
.mui-icon-right-nav{
	color: #FFFFFF;
}
a.mui-active {
	background: -webkit-linear-gradient(#9FBB4E, #547B10);
	background: -moz-linear-gradient(#9FBB4E, #547B10);
	background: -o-linear-gradient(#9FBB4E, #547B10);
	background: -webkit-gradient(linear, 0 0, 0 100%, from(#9FBB4E), to(#547B10));
	background: linear-gradient(#9FBB4E, #547B10);
	/* Opera 11.1 - 12.0 */
	/* Firefox 3.6 - 15 */
	/* 标准的语法 */
}

.mui-action-back{
	color: wheat;
}
.mui-table-view-cell::after{
	background:#9FBB4E ;
}

.mui-tab-label {
	color: #FFFFFF;
}
.mui-btn{
	color: white;
	background: #9FBB4E;
}
.mui-title{
	color: #FFFFFF;
	font-weight:bold;
}
.title {
	margin: 0px;
	padding: 10px;
	color: #000;
	font-weight: bold;
	font-size: 15px;
	background-color: #AECA5B;
}

.title >.more {
	color: #007aff;
	float: right;
}
.mui-input-group .mui-input-row::after{
	background: #ACC659;
}
